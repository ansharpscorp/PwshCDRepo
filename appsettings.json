{
  "GraphApi": {
    "BaseUrl": "https://graph.microsoft.com/v1.0/communications/callRecords",
    "TenantId": "your-tenant-id",
    "ClientId": "your-client-id",
    "ClientSecret": "your-client-secret",
    "Scope": "https://graph.microsoft.com/.default"
  },
  "DownloaderConfig": {
    "MaxDegreeOfParallelism": 4,
    "RetryCount": 5,
    "RetryBackoffSeconds": 5,
    "TimeoutSeconds": 100
  },
  "Paths": {
    "InputCsvFolder": "Input",
    "OutputFolder": "Output",
    "FailedCsvFolder": "Failed",
    "LogFolder": "Logs"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Custom": {
      "LogFileName": "DownloaderLog.txt"
    }
  }
}
